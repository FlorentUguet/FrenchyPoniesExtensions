{
	"name": "Frenchy Ponies CB Ignore",
	"version": "1.0",
	"manifest_version": 2,
	"description": "Ignore des personnes sur la CB Frenchy Ponies",
	
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
  
	"permissions": [
		"tabs", 
		"<all_urls>",
		"background"
	]
}